    "import subprocess\n",
    "\n",
    "args = [\"ping\", \"www.yahoo.com\"]\n",
    "process = subprocess.Popen(args, stdout=subprocess.PIPE)\n",
    "\n",
    "data = process.communicate()\n",
    "for line in data:\n",
    "    print(line)"
